{
  "name": "mcb-integration-layer",
  "version": "1.0.0",
  "description": "Mcb Integration Layer",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:unit && npm run test:acceptance",
    "test:unit": "cross-env ADMIN_SERVICE_URL=http://localhost ADMIN_SERVICE_JWT=abc mocha --require ts-node/register \"tests/unit/**/*.test.ts\" --timeout 8000 --exit",
    "test:acceptance": "mocha --require ts-node/register \"tests/acceptance/**/*.test.ts\" --timeout 8000 --exit",
    "start": "ts-node src/index.ts",
    "start:dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "fmt": "npm run lint-fix && npm run prettier-format",
    "prettier-format": "prettier --config .prettierrc \"./**/*.{ts,test.ts}\" --write",
    "prettier-check": "prettier --config .prettierrc \"./**/*.{ts,test.ts}\" --check",
    "commitlint-check": "commitlint",
    "prisma:generate": "npx prisma generate --schema=./prisma/schema.prisma",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:prod": "npx prisma migrate deploy",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://git@bitbucket.org/calmisland/mcb-integration-layer.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://bitbucket.org/calmisland/mcb-integration-layer/issues"
  },
  "homepage": "https://bitbucket.org/calmisland/mcb-integration-layer#readme",
  "dependencies": {
    "@apollo/client": "3.5.6",
    "@newrelic/winston-enricher": "2.1.0",
    "@prisma/client": "3.5.0",
    "bee-queue": "1.4.0",
    "cross-fetch": "3.1.4",
    "dotenv": "10.0.0",
    "express": "4.17.3",
    "graphql": "16.1.0",
    "newrelic": "8.4.0",
    "query-string": "7.0.1",
    "redis": "3.1.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@commitlint/cz-commitlint": "15.0.0",
    "@types/chai": "4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/chai-json-schema": "1.4.6",
    "@types/chai-spies": "1.0.3",
    "@types/express": "4.17.13",
    "@types/http-errors": "1.8.1",
    "@types/mocha": "9.0.0",
    "@types/newrelic": "7.0.2",
    "@types/node": "16.11.8",
    "@types/redis": "2.8.32",
    "@types/supertest": "^2.0.11",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "chai-json-schema": "1.5.1",
    "chai-spies": "1.0.0",
    "commitizen": "4.2.4",
    "cross-env": "7.0.3",
    "eslint": "8.2.0",
    "eslint-config-node": "4.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "inquirer": "8.2.0",
    "joi": "17.4.2",
    "mocha": "9.1.3",
    "msw": "0.36.3",
    "nock": "13.2.1",
    "nodemon": "2.0.15",
    "prettier": "2.4.1",
    "prisma": "3.5.0",
    "standard-version": "9.3.2",
    "supertest": "6.1.6",
    "ts-node": "10.4.0",
    "typescript": "4.5.2"
  }
}
